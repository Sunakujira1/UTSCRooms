{
  "name": "utscrooms",
  "version": "1.0.0",
  "description": "Find closest classrooms at UTSC that is available to use for study",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "npm run build:prod",
    "clean:build": "rimraf build",
    "prebuild:dev": "npm run clean:build",
    "build:dev": "webpack --config webpack.dev.config.js --progress --profile --colors --display-error-details",
    "prebuild:prod": "npm run clean:build",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --optimize-minimize --optimize-occurence-order",
    "server:dev": "webpack-dev-server --config webpack.dev.config.js --hot --inline --progress --profile --colors --display-error-details --content-base / --history-api-fallback",
    "start": "npm run server:dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sunakujira1/UTSCRooms.git"
  },
  "author": "Yuya Iwabuchi",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@horizon/client": "1.0.1",
    "babel-core": "6.7.4",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.2",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "google-map-react": "^0.19.0",
    "html-webpack-plugin": "2.14.0",
    "imports-loader": "0.6.5",
    "leaflet": "^0.7.7",
    "localforage": "1.4.0",
    "material-ui": "^0.16.0",
    "node-sass": "3.4.2",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-hot-loader": "1.3.0",
    "react-leaflet": "^0.12.1",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-router-redux": "2.1.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "3.3.1",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
